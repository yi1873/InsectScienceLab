<script type="text/javascript">
 function initializeMap(lat, long, message) {
	 var map;
    
    var myLatlng = new google.maps.LatLng(lat, long);
    var myOptions = {
      zoom: 7,
      center: myLatlng,
      mapTypeId: google.maps.MapTypeId.TERRAIN
      
    };
    
   map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);
   //center = myLatLong;
    var infowindow = new google.maps.InfoWindow({ 
    	//content:message + long + lat,
   	     //   size: new google.maps.Size(50,50)
   	     });
    
    var marker = new google.maps.Marker({
        position: myLatlng, 
        map: map, 
        title:message
    }); 
   google.maps.event.addListener(marker, 'click', function() {
       infowindow.open(map,marker);
   });

  }
</script>

<script type="text/javascript">
       function initializeGeocode() {
        	var address = (document.getElementById('geocodeMe'));
        	//alert("address: " + address);
        	var autocomplete = new google.maps.places.Autocomplete(address);
        	autocomplete.setTypes(['geocode']);
        	google.maps.event.addListener(autocomplete, 'place_changed', function() {
            	var place = autocomplete.getPlace();
            	if (!place.geometry) {
                	return;
           		 }

        	var address = '';
        	if (place.address_components) {
            	address = [
                	(place.address_components[0] && place.address_components[0].short_name || ''),
                	(place.address_components[1] && place.address_components[1].short_name || ''),
                	(place.address_components[2] && place.address_components[2].short_name || '')
                ].join(' ');
        	}
     	});
	}

	function codeAddress() {
    	geocoder = new google.maps.Geocoder();
    	
    	if(!document.getElementById("geocodeMe").value) {
    		alert("You did not enter an address");
    	} else {
    		var address = document.getElementById("geocodeMe").value;
    		geocoder.geocode( { 'address': address}, function(results, status) {
      			if (status == google.maps.GeocoderStatus.OK) {

      				if(results[0].geometry.location.lat() != "") {
      					alert("Latitude: "+roundToFour(results[0].geometry.location.lat()) );
      					alert("Longitude: "+roundToFour(results[0].geometry.location.lng()) );
      				}
      			}  else {
        		alert("Geocode was not successful for the following reason: " + status);
      			}
    		});
    	}
  	}
	
google.maps.event.addDomListener(window, 'load', initializeGeocode);

function roundToFour(num) {    
    return +(Math.round(num + "e+4")  + "e-4");
}
 </script>

<script>

	$(document).ready(function(){
		
		




		//JQUERY ecosystem selector elements
		$("#EcosystemCategory").remoteChained({
	    		parents : "#ecosystem",
	    		url : "rest/ecoselects/ecosystemcatV2"
	    		//clear : true
	    });

	   	 $("#EcosystemType").remoteChained({
	    	parents : "#ecosystem, #EcosystemCategory",
	    	url : "rest/ecoselects/ecosystemtypeV2"
	    	//clear : true
	    });

	    $("#EcosystemSubtype").remoteChained({
	    	parents : "#ecosystem, #EcosystemCategory, #EcosystemType",
	    	url : "rest/ecoselects/ecosystemsubtypeV2"
	    	//clear : true
	    });

	    $("#SpecificEcosystem").remoteChained({
	    	parents : "#ecosystem, #EcosystemCategory, #EcosystemType, #EcosystemSubtype",
	    	url : "rest/ecoselects/specificecosystemV2"
	    	//clear : true
	    });
	    
	    //---END ecosystem selectors
	    
	    //JQUERY new path ecosystem selector elements
	    //for the admin page to add new paths
		$("#addEcosystemCategory").remoteChained({
	    		parents : "#addEcosystem",
	    		url : "rest/ecoselects/ecosystemcatAdd",
	    		//clear : true,
	    		loading : "Loading..."
	    	});

	   	 $("#addEcosystemType").remoteChained({
	    	parents : "#addEcosystem, #addEcosystemCategory",
	    	url : "rest/ecoselects/ecosystemtypeAdd",
	    	//clear : true,
	    	loading : "Loading..."
	    });

	    $("#addEcosystemSubtype").remoteChained({
	    	parents : "#addEcosystem, #addEcosystemCategory, #addEcosystemType",
	    	url : "rest/ecoselects/ecosystemsubtypeAdd",
	    	//clear : true,
	    	loading : "Loading..."
	    });

	    $("#addSpecificEcosystem").remoteChained({
	    	parents : "#addEcosystem, #addEcosystemCategory, #addEcosystemType, #addEcosystemSubtype",
	    	url : "rest/ecoselects/specificecosystemAdd",
	    	//clear : true,
	    	loading : "Loading..."
	    });
	    
	    //---END new path ecosystem selectors
	
	});
	
	
</script>

