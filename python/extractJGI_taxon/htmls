	                    location.reload(true);
	        },
	        	error: function(xhr) {
	        		//alert("MSG " + xhr.responseText);
	        		$('#formMsg').html("<span class=\"formMsg\"><br /> Submission failed - "+ xhr.responseText +"</span>")
	        	}
	    	});
	    	
	    	return false;

		},
		
		submitTemperatureToWS: function (temperatureUnit, growthTemperature) {
			var url = 'rest/inlineedit/updateGrowthTemperature';
			//alert("TEMP: " + temperatureUnit + " -- unit: " + growthTemperature);
	    	$.ajax({
	    		url: url,
	    		type: 'put',
	    		dataType: 'text',
				data: "temperatureUnit=" + temperatureUnit + "&growthTemperature=" + growthTemperature + "&organismId=471766",
	    		success: function(res, sta, xhr) {
	    			   //$('#formMsg').html("<span class=\"formMsg\">" +
	                   //		"<br />"+ res +"</span>");

	    				//$( "#formMsg" ).ScrollTo();
	                    location.reload(true);
	        },
	        	error: function(xhr) {
	        		//alert("MSG " + xhr.responseText);
	        		$('#formMsg').html("<span class=\"formMsg\"><br /> Submission failed - "+ xhr.responseText +"</span>")
	        	}
	    	});
	    	
	    	return false;

		},
		
		submitDepthRangeToWS: function (depth, depth2) {
			
			//alert("DEPTH3: " + depth);
			
			var url = 'rest/inlineedit/doDepthRange';
			
	    	$.ajax({
	    		url: url,
	    		type: 'post',
	    		dataType: 'text',
				//data: $('#submitDepthRange').serialize(),
				data: "depth="+ depth + "&depth2=" + depth2 + "&organismId=" + 471766,
	    		success: function(res, sta, xhr) {
	    				$("#formMsg").ScrollTo();
					   var obj = JSON.parse(res);
					   $('#formMsg').html("<span class=\"formMsg\">" +
			               		"<br />"+ obj.msg +"</span>");
					   //alert("Made it");
			           	//reload the page from the server...
			           $('#formMsg').html("<span class=\"formMsg\">Reloading Page </span>");
			           	startSpinnner();
			            setTimeout(function (){
			            	location.reload(true); 
			            }, 1500);
			            
	        },
	        	error: function(xhr) {
	        		//alert("MSG " + xhr.responseText);
	        		$('#formMsg').html("<span class=\"formMsg\"><br /> Submission failed - "+ xhr.responseText +"</span>")
	        	}
	    	});
			
	    	
	    	return false;
		},
		
		submitAltRangeToWS: function () {
			var url = 'rest/inlineedit/doAltRange';
