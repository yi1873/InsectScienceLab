<script>





$(document).ready(function(){

    $(document).ready(function(){

    	
    	//ECOSYSTEMS
    	$( "#submitEcosystem" ).validate({
    			rules: {
    				"Ecosystem": "required", 
    				"Ecosystem Category": "required"
    			},

    			submitHandler: function(form) {
    				//alert("RES:: Here");
    				
    			$( "#submitEcosystem" ).submitToEcosystemWS();
    	  		},
    	  		errorClass: "validationErrorClass"
    		
    	});

    });

	
	//DISEASES
	$(".remDisease").on('click', function(){ 
		if(confirm("Remove " + this.id + " disease from the Organsim")){
			var divToRemove = "#disease"+this.id;
			$(divToRemove).remove();
			$(this).deleteElement(this.id, 'disease', 'rest/inlineedit/delOrganismMultiElement');
		}
	});
	
	 $('#addNewDisease').live('click', function(){
	  		var $this = $(this),
	      	$parentTR = $this.closest('tr');
	  		$parentTR.clone().insertAfter($parentTR);
	}); 

	$("#addNewDiseaseButton").on('click', function() {

		var url = 'rest/inlineedit/addOrganismDisease';
		var appendButton = addNewDiseaseButton;
		var dataFormId = addNewDiseases;
		$("#addNewDiseaseButton").submitNewElementToWS(url, appendButton, dataFormId);
    });

	//PHENOTYPES
	$(".remPhenotype").on('click', function(){ 
		if(confirm("Remove " + this.id + " phenotype from the Organsim")){
			var divToRemove = "#phenotype"+this.id;
			$(divToRemove).remove();
			$(this).deleteElement(this.id, 'phenotype', 'rest/inlineedit/delOrganismMultiElement');
		}
	});
	$('#addNewPhenotype').live('click', function(){
	  		var $this = $(this),
	      	$parentTR = $this.closest('tr');
	  		$parentTR.clone().insertAfter($parentTR);
	});
	$("#addNewPhenotypeButton").on('click', function() {

		var url = 'rest/inlineedit/addOrganismPhenotype';
		var appendButton = addNewPhenotypeButton;
		var dataFormId = addNewPhenotypes;
		$("#addNewPhenotypeButton").submitNewElementToWS(url, appendButton, dataFormId);
    });

	//METABOLISM
	$(".remMetabolism").on('click', function(){ 
		if(confirm("Remove " + this.id + " metabolism from the Organsim")){
			var divToRemove = "#metabolism"+this.id;
			$(divToRemove).remove();
			$(this).deleteElement(this.id, 'metabolism', 'rest/inlineedit/delOrganismMultiElement');
		}
	});
	$('#addNewMetabolism').live('click', function(){
	  		var $this = $(this),
	      	$parentTR = $this.closest('tr');
	  		$parentTR.clone().insertAfter($parentTR);
	});
	$("#addNewMetabolismButton").on('click', function() {

		var url = 'rest/inlineedit/addOrganismMetabolism';
		var appendButton = addNewMetabolismButton;
		var dataFormId = addNewMetabolisms;
		$("#addNewMetabolismButton").submitNewElementToWS(url, appendButton, dataFormId);
    });


	//BODY PRODUCTS
	$(".remBodyProducts").on('click', function(){ 
		if(confirm("Remove " + this.id + " Body Product from the Organsim")){
			var divToRemove = "#bodyproducts"+this.id;
			$(divToRemove).remove();
			$(this).deleteElement(this.id, 'bodyproducts', 'rest/inlineedit/delOrganismMultiElement');
		}
	});
	$('#addNewBodyProducts').live('click', function(){
	  		var $this = $(this),
	      	$parentTR = $this.closest('tr');
	  		$parentTR.clone().insertAfter($parentTR);
	});
	$("#addNewBodyProductsButton").on('click', function() {

		var url = 'rest/inlineedit/addOrganismBodyProducts';
		var appendButton = addNewBodyProductsButton;
		var dataFormId = addNewBodyProduct;
		$("#addNewBodyProductsButton").submitNewElementToWS(url, appendButton, dataFormId);
		
    });

	//ENERGY SORCES
	$(".remEnergySources").on('click', function(){ 
		if(confirm("Remove " + this.id + " Energy Source from the Organsim")){
			var divToRemove = "#energysources"+this.id;
			$(divToRemove).remove();
			$(this).deleteElement(this.id, 'energysources', 'rest/inlineedit/delOrganismMultiElement');
		}
	});
	$('#addNewEnergySources').live('click', function(){
	  		var $this = $(this),
	      	$parentTR = $this.closest('tr');
	  		$parentTR.clone().insertAfter($parentTR);
	});
	$("#addNewEnergySourcesButton").on('click', function() {

		var url = 'rest/inlineedit/addOrganismEnergySources';
		var appendButton = addNewEnergySourcesButton;
		var dataFormId = addNewEnergySource;
		$("#addNewEnergySourcesButton").submitNewElementToWS(url, appendButton, dataFormId);
    });

	//Cell Arrangements
	$(".remCellArrangements").on('click', function(){ 
		if(confirm("Remove " + this.id + " Cell Arrangement from the Organsim")){
			var divToRemove = "#cellarrangements"+this.id;
			$(divToRemove).remove();
			$(this).deleteElement(this.id, 'cellarrangement', 'rest/inlineedit/delOrganismMultiElement');
		}
	});
	$('#addNewCellArrangements').live('click', function(){
	  		var $this = $(this),
	      	$parentTR = $this.closest('tr');
	  		$parentTR.clone().insertAfter($parentTR);
	});
	
	$("#addNewCellArrangementsButton").on('click', function() {

		var url = 'rest/inlineedit/addOrganismCellArrangements';
		var appendButton = addNewCellArrangementsButton;
		var dataFormId = addNewCellArrangement;
		$("#addNewCellArrangementsButton").submitNewElementToWS(url, appendButton, dataFormId);

    });

	//Biotic RelationshipS
	$(".remBioticRelationship").on('click', function(){ 
		if(confirm("Remove " + this.id + " Biotic Relationship from the Organsim")){
			var divToRemove = "#BioticRelationship"+this.id;
			$(divToRemove).remove();
			$(this).deleteElement(this.id, 'bioticrelationship', 'rest/inlineedit/delOrganismMultiElement');
		}
	});
	$('#addNewBioticRelationships').live('click', function(){
	  		var $this = $(this),
	      	$parentTR = $this.closest('tr');
	  		$parentTR.clone().insertAfter($parentTR);
	});
	
	$("#addNewBioticRelationshipButton").on('click', function() {

		var url = 'rest/inlineedit/addOrganismBioticRelationships';
		var appendButton = addNewBioticRelationshipButton;
		var dataFormId = addNewBioticRelationship;
		$("#addNewBioticRelationshipButton").submitNewElementToWS(url, appendButton, dataFormId);

    });

	//HABITATS
	$(".remHabitat").on('click', function(){ 
		if(confirm("Remove " + this.id + " Habitat from the Organsim")){
			var divToRemove = "#Habitat"+this.id;
			$(divToRemove).remove();
			$(this).deleteElement(this.id, 'habitat', 'rest/inlineedit/delOrganismMultiElement');
		}
	});
	$('#addNewHabitat').live('click', function(){
	  		var $this = $(this),
	      	$parentTR = $this.closest('tr');
	  		$parentTR.clone().insertAfter($parentTR);
	});
	
	$("#addNewHabitatButton").on('click', function() {

		var url = 'rest/inlineedit/addOrganismElements';
		var appendButton = addNewHabitatButton;
		var dataFormId = addNewHabitats;
		var classId = 'habitatid';
		$('<input>').attr({type: 'hidden',id: 'elementTable',name: 'elementTable', 
			value: 'habitat'}).appendTo(appendButton);
		$("#addNewHabitatButton").submitNewElementToWS(url, appendButton, dataFormId, classId);

    });
	
	
	$("[id^=doDepthRangeSubmit]").on("click", function() {
		
		var submitElId = $(this).attr('id');
		var depth;
		var depth2;
		
		if(submitElId == "doDepthRangeSubmit") {
		
			depth = $("#depth").val();
			depth2 = $("#depth2").val();
			
		} else if(submitElId == "doDepthRangeSubmitSoil") {
			depth = $("#soilDepth").val();
			depth2 = $("#soilDepth2").val();
		}
		
		if(!$("#doDepthRangeSubmit").checkAltElDepthValues(depth, depth2)) {
    		return false;
    	 } else {
			$("#submitDepthRange").submitDepthRangeToWS(depth, depth2);
    		
    	 }
		return false;
		
		
	});
	
	//add alt range
	$("#doAltRangeSubmit").on("click", function() {
		var alt = $("#altitude").val();
		var alt2 = $("#altitude2").val();
		
		if(!$("#doAltRangeSubmit").checkAltElDepthValues(alt, alt2)) {
			//alert("No GOOD");
    		return false;
    	 } else {
    		 
			$("#submitAltRange").submitAltRangeToWS();
    	 }
		return false;
		
		
		
	});
	
	$("[id^=doElRangeSubmit]").on("click", function() {
		
		var el;
		var el2;
		var submitElId = $(this).attr('id');
		
		if(submitElId == "doElRangeSubmit") {
			el = $("#elevation").val();
			el2 = $("#elevation2").val();
		} else if(submitElId == "doElRangeSubmitSoil") {
			el = $("#soilElevation").val();
			el2 = $("#soilElevation2").val();
		}
		//alert("Update elevation");
		if(!$("#doElRangeSubmit").checkAltElDepthValues(el, el2)) {
			//alert("No GOOD");
    		return false;
    	 } else {
    		 //alert("Update elevation2");
			$("#submitElRange").submitElRangeToWS(el, el2);
    	 }
		return false;
		
	});
		
	
	//update the temp field
	$("#updateTemperature").on("click", function() {
		var temperatureUnit = $('select[id="temperatureUnit"]').val();
		var growthTemperature = $('input[id="GrowthTemperature"]').val();
		$("#updateTemperatureForm").submitTemperatureToWS(temperatureUnit, growthTemperature);
	});
	
	$("#submitOrganismPubMedId").on('click', function() {
		$("#submitOrganismPubMedId").submitNewPubmedIdToWS();
    });
	
	$("#uploadImageButton").on('click', function() {
		//imageSelectionPopup();
		$( "#photo-upload-dialog" ).dialog({
		      resizable: false,
		      height: 350,
		      width: 400,
		      modal: true,
		      show: {
		          effect: "fade",
		          duration: 500
		        },
		        hide: {
		          effect: "fade",
		          duration: 1000
		        },
		      buttons: {
		        "Upload Image": function() {
		          alert("Upload Image");
		          $( "#photoUploadForm" ).submit();
		          $( this ).dialog( "close" );
		        },
		        Cancel: function() {
		          $( this ).dialog( "close" );
		        }
		      }
		 });
	});
	
	
	//image uploader
	 function imageSelectionPopup() {
    	  var w = window.open('', '', 'width=500,height=300,top=100,left=150,resizeable,'
    			  +'scrollbars,location=0,toolbar=0');
    	  w.document.write("<html><body>");
    	  w.document.write("<table height=\"95%\" cellpadding=\"5\">");
    	  w.document.write("<tr>");
    	  w.document.write("<td width=\"30%\" bgcolor=\"yellow\" rowspan=\"2\">");
    	  w.document.write("side");
    	  w.document.write("</td>");
    	  w.document.write("<td bgcolor=\"#CCCCCC\">");
    	  w.document.write("Image selection and upload form");
    	  w.document.write("<form action=\"uploadImage\" method=\"post\" enctype=\"multipart/form-data\">");
    	  w.document.write("<input type=\"file\" name=\"file\" /> <br\><br\>");
    	  w.document.write("<input type=\"hidden\" name=\"organismId\" id=\"organismId\" value=\"471766\" />");
    	  w.document.write("<input type=\"submit\" value=\"Upload image\" onSubmit=\"javascript: self.close()\"/>");
    	  w.document.write("</form>");
    	  w.document.write("</td>");
    	  w.document.write("</tr>");
    	  w.document.write("</table>");
    	  w.document.write("</body></html>");
    	  w.document.close(); 
    }
    
	//progressbar
	var progressbar = $( "#progressbar" ),
      progressLabel = $( ".progress-label" );
 
    progressbar.progressbar({
      value: false,
      change: function() {
        progressLabel.text( progressbar.progressbar( "value" ) + "%" );
      },
      complete: function() {
        progressLabel.text( "Complete!" );
      }
    });

    
    function progress() {
      var val = progressbar.progressbar( "value" ) || 0;
 
      progressbar.progressbar( "value", val + 2 );
 
      if ( val < 99 ) {
        setTimeout( progress, 80 );
      }
    }
 
    setTimeout( progress, 2000 );
	///
	});

jQuery.fn.extend({

		deleteElement: function (data, tableName, url) {
							
							$.ajax({
								url: url,
								type: 'put',
								dataType: 'text',
								data: "elementValue=" + data + "&elementTable=" + tableName + "&organismId=471766",
								success: function(res, sta, xhr) {
							   // alert("RES:: " + res);
							   $("#header").ScrollTo(0);
							   $('#formMsg').html("<span class=\"formMsg\">" +
					               		"<br />"+ res +"</span>");
							   
					           	//reload the page from the server...
					           $('#formMsg').html("<span class=\"formMsg\">Reloading Page </span>");
					    	       	startSpinnner();
						            setTimeout(function (){
						           	location.reload(true); 
					            }, 1500);
									                          
								},
								error: function(xhr) {
									//alert("MSG " + xhr.responseText);
									$("#header").ScrollTo(0);
									$('#formMsg').html("<span class=\"formMsg\"><br /> Submission failed - "+ xhr.responseText +"</span>");
									//$( "#formMsg" ).ScrollTo();
								}
							});

							return false;
							},

		submitNewElementToWS: function(url, appendButton, dataFormId, className) {

					$('<input>').attr({type: 'hidden',id: 'organism_id',name: 'organism_id', 
						value: 471766}).appendTo(appendButton);
					//get field that is the new element name and append as a elementValue param
					var elList = $( dataFormId ).find('.' + className + ' option:selected').map(function() {return this.value;}).get().join(',');
					//alert("element:" + elList);
					$('<input>').attr({type: 'hidden',id: 'elementValues',name: 'elementValues', value: elList}).appendTo(dataFormId);
					
					$.ajax({
						url: url,
						type: 'put',
						dataType: 'text',
						data: $(dataFormId).serialize(),
						success: function(res, sta, xhr) {
						   $("#formMsg").ScrollTo();
						   //alert("RES:: " + res);
						   $('#formMsg').html("<span class=\"formMsg\">" +
				               		"<br />"+ res +"</span>");
						   
				           	//reload the page from the server...
				           $('#formMsg').html("<span class=\"formMsg\">Reloading Page </span>");
				           	startSpinnner();
				            setTimeout(function (){
				            	location.reload(true); 
				            }, 1500);
				                          
					},
						error: function(xhr) {
						//alert("MSG " + xhr.responseText);
						 $('#formMsg').html("<span class=\"formMsg\"><br /> Submission failed - "+ xhr.responseText +"</span>");
						 $( "#formMsg" ).ScrollTo();
						}
					});

					return false;

				},

		submitToEcosystemWS: function () {

			    		//alert("RES:: Here2");
			    		
			    	var url = 'rest/inlineedit/EcosystemsV2';

			    	$.ajax({
			    		url: url,
			    		type: 'put',
			    		dataType: 'text',
			    		data: $('#submitEcosystem').serialize(),
			    		success: function(res, sta, xhr) {
			    			   // alert("RES:: " + res);
			    			   //
			    				$('#ecoFormMsg').html("<span class=\"ecoFormMsg\">" +
			                   		"<br />"+ res +"</span>");

			    				$( "#ecoFormMsg" ).ScrollTo();

			                   	//reload the page from the server...
			                    location.reload(true);
			                    
			        },
			        	error: function(xhr) {
			        		//alert("MSG " + xhr.responseText);
			        		$('#ecoFormMsg').html("<span class=\"ecoFormMsg\"><br /> Submission failed - "+ xhr.responseText +"</span>")
			        	}
			    	});
			    	
			    	
			    	
			    	return false;

			    		
			    	//});

		},
		
		submitNewPubmedIdToWS: function () {
			var url = 'rest/inlineedit/addOrganismPubMedId';

	    	$.ajax({
	    		url: url,
	    		type: 'put',
	    		dataType: 'text',
